<div class="container">

<h1>Login</h1>

<%= bootstrap_form_for :user do |f| %>
    <%= f.text_field :email, placeholder: "Enter your email" %>
    <%= f.password_field :password, placeholder: "Enter your password"%>
    <!-- < %= f.primary "Login"% > -->
    <%= f.image_submit_tag("Coffee.png") %>
<% end %>

</div>